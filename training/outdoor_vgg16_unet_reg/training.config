[model]
folder = ./custom_models/unet_vgg16
activation = sigmoid
classes = 1

[parameters]
epochs = 1000
batch_size = 8
size = (256, 512)

[split]
test = 0.2
train = 0.8
valid = 0.2
split = train_test_split
seed = 42

[mapping]
mapping = image_histogram_mapping_segmentation
uv = False
normalize = False
reg_head = False
weighted = True

[train]
dataset_type = newcube2
dataset_path = /media/donik/Disk/Cube2_new
list_name = list_outdoor.txt
regression = False
return_gt = True
corrected = False
round = False
size = ${parameters:size}
batch_size = ${parameters:batch_size}

[valid]
dataset_type = newcube2
dataset_path = /media/donik/Disk/Cube2_new
list_name = list_outdoor.txt
regression = False
return_gt = True
round = False
corrected = False
size = ${parameters:size}
batch_size = ${parameters:batch_size}

[test]
dataset_type = newcube2
dataset_path = /media/donik/Disk/Cube2_new
list_name = list_outdoor.txt
regression = False
return_gt = True
round = False
corrected = False
size = ${parameters:size}
batch_size = 1

[loss]
name = UNetLoss
seg_dimensions = 1
reg_dimensions = 3
reg_head = ${mapping:reg_head}
multi_ill = False
seg_loss = regression

[optimizer]
name = AdamW
weight_decay=1e-7
learning_rate=1e-5
beta_1=0.9
beta_2=0.999

[metrics]
names = MeanAbsoluteError