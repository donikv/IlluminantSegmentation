[model]
folder = ./custom_models/unet_vgg16_clustering
classes = 1
activation = relu
gamma = 3

[parameters]
epochs = 500
batch_size = 2
size = (256, 512)

[split]
test = 0.2
train = 0.8
valid = 0.2
split = train_test_split
seed = 42

[mapping]
mapping = image_histogram_mapping_segmentation
uv = False
normalize = False
reg_head = False
multioutput = False
weighted = True
clustering=True
correct=False

[input_mapping]
mapping = multioutput_labels_mapping
count = 1

[train]
dataset_type = newcube2
dataset_path = /media/donik/Disk/Cube2_new_
list_name = list_outdoor.txt
regression = False
return_gt = True
return_gt_mask = False
corrected = False
round = False
size = ${parameters:size}
batch_size = ${parameters:batch_size}

[valid]
dataset_type = newcube2
dataset_path = /media/donik/Disk/Cube2_new_
list_name = list_outdoor.txt
regression = False
return_gt = True
return_gt_mask = False
corrected = False
round = False
size = ${parameters:size}
batch_size = ${parameters:batch_size}

[test]
dataset_type = newcube2
dataset_path = /media/donik/Disk/Cube2_new_
list_name = list_outdoor.txt
regression = False
return_gt = True
return_gt_mask = False
corrected = False
round = False
size = ${parameters:size}
batch_size = 1

[loss]
name = MAE


[optimizer]
name = AdamW
weight_decay=1e-7
learning_rate=1e-5
beta_1=0.99
beta_2=0.999

[metrics1]
names = MeanAbsoluteError
